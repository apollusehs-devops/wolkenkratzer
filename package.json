{
  "name": "wolkenkratzer",
  "version": "0.6.0",
  "description": "Library for generating CloudFormation templates",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "aws-sdk": "^2.3.17",
    "js-yaml": "^3.6.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "browserify": "^14.1.0",
    "fs-extra": "^2.0.0",
    "lodash": "^4.13.1",
    "markdown-toc": "^1.1.0",
    "nyc": "^10.1.2",
    "prettier": "^0.22.0",
    "typescript": "^2.2.1",
    "x-ray": "^2.3.0"
  },
  "scripts": {
    "test": "time tsc && nyc --cache --reporter=lcov ava --verbose && nyc report",
    "builddocs": "jsdoc -c jsdoc.conf.json -R ./README.md",
    "publishdocs": "git push origin `git subtree split --prefix docs/ master`:gh-pages --force",
    "linecount": "cloc --vcs=git ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arminhammer/wolkenkratzer.git"
  },
  "keywords": [
    "CloudFormation",
    "AWS",
    "Template",
    "Cloud",
    "aws-sdk",
    "ecs"
  ],
  "author": "Armin Graf",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/arminhammer/wolkenkratzer/issues"
  },
  "homepage": "https://github.com/arminhammer/wolkenkratzer#readme",
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "concurrency": 32,
    "failFast": false,
    "tap": false,
    "powerAssert": false
  }
}